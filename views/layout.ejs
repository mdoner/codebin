<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
     <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
     <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

    <% if(data.input != 'cx') {%>
      <!-- OG Meta Share -->
      <meta property="og:title" content="Codebin by: <%= data.name %> | <%= data.id %>"/>
      <meta property="og:description" content="<%= data.input.toString().substr(0,30) %>..."/>
      <meta property="og:type" content="website"/>
      <meta property="og:url" content="https://codebin.run/share/<%= data.id %>"/>
      <meta property="og:site_name" content="Codebin | Share your code!"/>
      <meta property="og:image" content="https://i.ntaheij.dev/codebin-1.png"/>
      <meta name="theme-color" content="#62B6AF">
      <meta name="keywords" content="hastebin, pastebin, code, share, codeshare, share code, code share, haste bin, hasteb.in, live code, html, php, css, js, javascript, share javascript, text editor, text, codebin.run, codebin.run/<%= data.id %>, <%= data.id %>">
      <meta name="robots" content="noindex,nofollow" />

    <%} else {%>
        <!-- OG Meta Normal-->
        <meta property="og:title" content="Codebin"/>
        <meta property="og:description" content="Want an easy place to share your code? Come right to Codebin!"/>
        <meta property="og:type" content="website"/>
        <meta property="og:url" content="https://codebin.run/"/>
        <meta property="og:site_name" content="Codebin | Share your code!"/>
        <meta property="og:image" content="https://i.ntaheij.dev/codebin-1.png"/>
        <meta name="theme-color" content="#62B6AF">
        <meta name="keywords" content="hastebin, pastebin, code, share, codeshare, share code, code share, haste bin, hasteb.in, live code, html, php, css, js, javascript, share javascript, text editor, text">
        <meta name="robots" content="noindex,nofollow" />
    <%}%>

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/png" href="https://i.ntaheij.dev/codebin-1.png">

    <!-- Styles -->
    <link
      rel="stylesheet"
      href="/media/extra.css"
    />
    <link rel="stylesheet" type="text/css" href="/media/solarized_dark.css">
    <link rel="stylesheet" type="text/css" href="/media/application.css">

    <!-- Scripts -->
    <script src="https://www.recaptcha.net/recaptcha/api.js" async defer></script>
    
    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script>
    <link rel="stylesheet" type="text/css" href="/media/application.min.js">
    <script type="text/javascript" src="/media/application.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.8.0/highlight.min.js"></script>
    <script>
      document.addEventListener('DOMContentLoaded', (event) => {
        document.querySelectorAll('pre code').forEach((block) => {
          hljs.highlightBlock(block);
        });
      });

      $(window).bind('keydown', function(event) {
          if (event.ctrlKey || event.metaKey) {
            console.log(String.fromCharCode(event.which).toLowerCase())
              switch (String.fromCharCode(event.which).toLowerCase()) {
              case 's':
                  event.preventDefault();
                  $("#submit").click();
                  break;
              case 'n':
                event.preventDefault();
                window.location.href = "/create";
                break;
              }
          }
      });
    </script>
    <script src="https://rawgit.com/jackmoore/autosize/master/dist/autosize.min.js"></script>
    <script>
      autosize(document.getElementById("input"));
    </script>
    <script>
      $(function(){var t=!0;$("textarea.tabSupport").keydown(function(e){if(27==e.keyCode)return t=!t,!1;if(13===e.keyCode&&t&&this.selectionStart==this.selectionEnd){for(var s=this.selectionStart,i=$(this).val();s>0&&"\n"!=i[s-1];)s--;for(var n=s;" "==i[s]||"\t"==i[s];)s++;if(s>n)return document.execCommand("insertText",!1,"\n"+i.substr(n,s-n)),this.blur(),this.focus(),!1}if(9===e.keyCode&&t){if(this.selectionStart==this.selectionEnd)if(e.shiftKey){i=this.value;this.selectionStart>0&&"\t"==i[this.selectionStart-1]&&document.execCommand("delete")}else document.execCommand("insertText",!1,"\t");else{var r=this.selectionStart,o=this.selectionEnd;for(i=$(this).val();r>0&&"\n"!=i[r-1];)r--;for(;o>0&&"\n"!=i[o-1]&&o<i.length;)o++;for(var h=i.substr(r,o-r).split("\n"),l=0;l<h.length;l++)l==h.length-1&&0==h[l].length||(e.shiftKey?h[l].startsWith("\t")?h[l]=h[l].substr(1):h[l].startsWith("    ")&&(h[l]=h[l].substr(4)):h[l]="\t"+h[l]);h=h.join("\n"),this.value=i.substr(0,r)+h+i.substr(o),this.selectionStart=r,this.selectionEnd=r+h.length}return!1}return t=!0,!0})});
    </script>
      
    <title>Codebin | Share your code!</title>
  </head>
  <body>
        <%- body %>
        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>
        <% include ./partials/footer %>
  </body>
</html>
