<!-- Header -->
<nav class="navbar navbar-expand-lg navbar-dark bg-dark noselect">
  <a class="navbar-brand" href="/">
      <img src="/media/codebin-1.png" width="40" height="40" alt="" loading="lazy">
    </a>
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <div class="collapse navbar-collapse" id="navbarColor01">
    <ul class="navbar-nav mr-auto">
      <a class="navbar-brand" href="/">
        <h2 class="align-text-bottom" style="color: #62B6AF;">Codebin by: <%= data.name %></h2>
      </a>
    </ul>
    <% if(name !== 'Anonymous') {%>
      <a href="/users/logout"><img style="width: 30px; height: 30px;" src="/media/logout.svg" loading="lazy"></a>
    <%}%>
  </div>
</nav>
<!-- Container -->
<div>
  <div id="linenos" style="font-size: 1.0rem;">
  <% for(var i = 0; i<data.input.length; i++) { %><%= Number(i)+1; %> </br><%}%>
  </div>
  <div style="margin-left: 1.1rem; ">
    <pre id="box" class="ml-4"><code class="hljs"><% for(i in data.input) { %><%= data.input[i] %></br><%}%></code></pre>
  </div>
</div>